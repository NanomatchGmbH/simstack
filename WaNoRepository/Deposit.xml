<WaNoTemplate name="Deposit">
    <WaNoSection name="Box" layout="horizontal">
        <WaNoFloat name="LX">
            0.25
        </WaNoFloat>
        <WaNoFloat name="LY">
            0.25
        </WaNoFloat>
        <WaNoFloat name="LZ">
            0.25
        </WaNoFloat>
    </WaNoSection>
    <WaNoVarSection name="System" min="1" max="5">
        <WaNoSection name="Molecule">
            <WaNoFile name="molecule">
                input.pdb
            </WaNoFile>
            <WaNoFloat name="fraction">1.0</WaNoFloat>
        </WaNoSection>
    </WaNoVarSection>
    <WaNoSection name="Protocol" >
        <WaNoInt name="NoCycles">     10</WaNoInt>
        <WaNoInt name="Steps">     20000</WaNoInt>
        <WaNoFloat name="TStart">  1000</WaNoFloat>
        <WaNoFloat name="TEnd">  1000</WaNoFloat>
        <WaNoSelection name="Forcefield" layout="auto">
            <WaNoSelectionElement>"AMBER"</WaNoSelectionElement>
            <WaNoSelectionElement>"AMBER99"</WaNoSelectionElement>
            <WaNoSelectionElement>"AMBER99ISDL"</WaNoSelectionElement>
            <WaNoSelectionElement>"AMBER99ISDL**"</WaNoSelectionElement>
        </WaNoSelection>
        <WaNoOutputFile required="True" name="outfile1" condition="filesize(outfile1) > 50.0"> 
            deposit.out
        </WaNoOutputFile>    
    </WaNoSection>
    </WaNoTemplate>
   

